<%- include("partials/header"); -%>


    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
        <label class="form-label form-group" for="postTitle">Title</label>
        <input class="form-control"  type="text" name="postTitle" autocomplete="off">

        <label for="postBody">Post</label>
        <textarea class="form-control "  name="postBody" autocomplete="off " rows="5" ></textarea>
        </div>
        <button type="submit" class="btn btn-primary ">Publish</button>
    </form>




<%- include("partials/footer"); -%>